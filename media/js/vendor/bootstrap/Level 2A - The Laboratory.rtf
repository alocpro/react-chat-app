\par \pard\plain \s46\ql\nowidctlpar\sb0\sa120\ltrpar\kerning1\dbch\af11\langfe2052\dbch\af14\afs24\alang1081\loch\f4\fs24\lang4105\alang1025\rtlch \ltrch\loch

